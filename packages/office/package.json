{
  "name": "mermaid-chart-office-addin",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite dev --port 3000",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "lint": "pnpm check && eslint . && prettier --check ./**/*.{svelte,ts,js,json,md,html}",
    "lint:fix": "eslint --fix . && prettier --write ./**/*.{svelte,ts,js,json,md,html}",
    "start": "office-addin-debugging start ./manifests/manifest.xml",
    "start:desktop": "office-addin-debugging start ./manifests/manifest.xml desktop",
    "start:web": "office-addin-debugging start ./manifests/manifest.xml web",
    "start:excel": "office-addin-debugging start ./manifests/manifest.excel.xml",
    "start:onenote": "office-addin-debugging start ./manifests/manifest.onenote.xml",
    "start:powerpoint": "office-addin-debugging start ./manifests/manifest.powerpoint.xml",
    "start:project": "office-addin-debugging start ./manifests/manifest.project.xml",
    "start:word": "office-addin-debugging start ./manifests/manifest.word.xml",
    "stop": "office-addin-debugging stop ./manifests/manifest.xml",
    "validate": "office-addin-manifest validate ./manifests/manifest.xml"
  },
  "dependencies": {
    "@badgateway/oauth2-client": "^2.2.3",
    "@floating-ui/dom": "^1.2.9",
    "@inqling/svelte-icons": "^2.1.0",
    "@logtail/pino": "^0.3.1",
    "@sveltejs/adapter-vercel": "3.0.1",
    "@types/uuid": "^9.0.1",
    "axios": "^1.4.0",
    "buffer": "^6.0.3",
    "html2canvas": "^1.4.1",
    "lodash-es": "^4.17.21",
    "luxon": "^3.1.1",
    "mermaid": "10.2.3",
    "office-addin-dev-certs": "^1.11.4",
    "openapi-types": "^12.1.3",
    "pino": "^8.11.0",
    "svelte-icons": "^2.1.0",
    "svelte-local-storage-store": "^0.6.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@cspell/eslint-plugin": "^6.19.2",
    "@skeletonlabs/skeleton": "^1.7.1",
    "@sveltejs/adapter-auto": "2.1.0",
    "@sveltejs/kit": "1.17.0",
    "@tailwindcss/forms": "^0.5.3",
    "@types/lodash-es": "^4.17.6",
    "@types/luxon": "^3.1.0",
    "@types/office-js": "^1.0.320",
    "@types/office-runtime": "^1.0.23",
    "@typescript-eslint/eslint-plugin": "^5.48.2",
    "@typescript-eslint/parser": "^5.48.2",
    "axios-logger": "^2.7.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.32.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-html": "^7.1.0",
    "eslint-plugin-jest": "^27.1.5",
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-markdown": "^3.0.0",
    "eslint-plugin-no-only-tests": "^3.1.0",
    "eslint-plugin-office-addins": "^2.1.5",
    "eslint-plugin-svelte": "^2.28.0",
    "eslint-plugin-unicorn": "^46.0.0",
    "jsdom": "^21.0.0",
    "office-addin-cli": "^1.5.5",
    "office-addin-debugging": "^5.0.10",
    "office-addin-dev-certs": "^1.11.3",
    "office-addin-dev-settings": "^2.0.8",
    "office-addin-lint": "^2.2.5",
    "office-addin-manifest": "^1.12.3",
    "office-addin-prettier-config": "^1.2.0",
    "postcss": "^8.4.14",
    "postcss-load-config": "^4.0.1",
    "svelte": "^3.59.1",
    "svelte-check": "^3.3.2",
    "svelte-eslint-parser": "^0.28.0",
    "svelte-preprocess": "^5.0.3",
    "tailwindcss": "^3.2.4",
    "typescript": "^4.9.4",
    "vite": "^4.3.7",
    "vitest": "^0.30.1"
  },
  "volta": {
    "node": "18.16.1"
  }
